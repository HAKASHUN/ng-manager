#entity-table-head
  table.head
    thead
      tr
        th(ng-repeat='field in fields' ng-style='field.style')
          span(ng-bind='field.id|translate')
        th.actions
#entity-table-body
  table.body
    tbody
      tr(ng-repeat='row in rows' fields='fields' row='row' schema='schema' entity-row='')

