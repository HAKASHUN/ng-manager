.entity-list

  .entity-list-header
    padding 0.4em 10px

    md-button
      padding 0.4em 1em
      margin 0.4em

  table
    table-layout fixed
    margin-bottom 0

    thead
      th
        border-bottom-width 1px

    th
      text-align left

    td
      text-overflow ellipsis
      white-space nowrap
      overflow hidden

  #entity-table-body
    overflow-y auto
